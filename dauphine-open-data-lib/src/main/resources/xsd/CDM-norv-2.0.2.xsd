			<xs:enumeration value="colloque"/>
			<xs:enumeration value="travauxDiriges"/>
			<xs:enumeration value="travauxPratiques"/>
			<xs:enumeration value="projetIndividuel"/>
			<xs:enumeration value="projetEnGroupe"/>
			<xs:enumeration value="travauxDeLaboratoire"/>
			<xs:enumeration value="etudeBibliographique"/>
			<xs:enumeration value="exercices"/>
			<xs:enumeration value="devoir"/>
			<xs:enumeration value="stage"/>
			<xs:enumeration value="correspondance"/>
		</xs:restriction>
